<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Expired_By_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF( RecordType.Name = &quot;Today&quot; &amp;&amp; DATEVALUE( CreatedDate ) &lt; TODAY(), true, IF(RecordType.Name = &quot;Tomorrow&quot; &amp;&amp; (DATEVALUE( CreatedDate ) + 1) &lt; TODAY(), true, false) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Expired By Date</label>
        <securityClassification>Public</securityClassification>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>External_Id__c</fullName>
        <externalId>false</externalId>
        <label>External Id</label>
        <length>100</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IsPersonal__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <label>IsPersonal</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Expired__c</fullName>
        <externalId>false</externalId>
        <formula>IF( RecordType.Name  = &quot;Today&quot;  &amp;&amp;   DATEVALUE( CreatedDate ) &lt;  TODAY() &amp;&amp;  NOT( Is_Done__c ) , true,  IF(RecordType.Name  = &quot;Tomorrow&quot;  &amp;&amp;   (DATEVALUE( CreatedDate ) + 1) &lt;  TODAY() &amp;&amp; NOT( Is_Done__c ), true, false) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Expired</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
